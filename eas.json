{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "withoutCredentials": "true",
        "gradleCommand": ":app:assembleDebug -PreactNativeArchitectures=arm64-v8a"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "BUNDLE_ID": "com.cherry_ai.cherry_studio_app"
        }
      },
      "ios": {
        "env": {
          "BUNDLE_ID": "com.cherry-ai.cherry-studio-app"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "distribution": "internal",
      "env": {
        "PROFILE": "production"
      },
      "android": {
        "buildType": "apk",
        "withoutCredentials": "true",
        "gradleCommand": ":app:assembleRelease -PreactNativeArchitectures=arm64-v8a"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6754246754"
      }
    }
  }
}
